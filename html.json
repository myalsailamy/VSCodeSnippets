	  "ramez-change-status-column": {
		"prefix": "ramez-change-status-column",
		"body": [
		  "<ng-container matColumnDef=\"${2:ColumnNameDownCase}\">",
		  " <mat-header-cell *matHeaderCellDef mat-sort-header=\"${2/(.)/${1:/downcase}/}\">{{'Common.CRUD.Enabled' | translate }}</mat-header-cell>",
		  " <mat-cell *matCellDef=\"let ${1:SingularEntityName}\" [attr.data-label]=\"'Common.CRUD.Enabled' | translate \">",
		  "   <mat-icon [color]=\"${1/(.)/${1:/downcase}/}.${2/(.)/${1:/downcase}/} == false ? 'primary' : 'warn'\"",
