	  "ramez-resolver-form": {
		"prefix": "ramez-resolver-form-component",
		"body": [
		  "<ng-container *ngIf=\"(${2:SingularEntityNameDowncase}Info$ | async) as ${2/(.)/${1:/downcase}/}Data; else loading\">",
		  "  <div fxLayout=\"column\" fxLayoutAlign=\"start stretch\" fxLayoutGap=\"10px\">",
		  "",
		  "    <!-- begin header form -->",
		  "    <mat-accordion #mainAccordion=\"matAccordion\" class=\"mat-accordion-primary\" displayMode=\"default\" [multi]=\"true\" [hideToggle]=\"false\">",
		  "",
		  "      <mat-expansion-panel [hideToggle]=\"false\" class=\"mat-elevation-z0\"  [expanded]=\"true\">",
		  "        <mat-expansion-panel-header fxLayout=\"row\" fxLayoutAlign=\"space-between center\">",
		  "          <mat-panel-title fxLayout=\"row\" fxLayoutAlign=\"start center\">",
		  "            <mat-icon fxFlex=\"none\" color=\"primary\">{{!isEdit ? 'add' : 'drive_file_rename_outline'}}</mat-icon>",
		  "            <h2 fxFlex class=\"dialog-title\" mat-dialog-title style=\"margin-bottom: 0px;\">",
		  "              {{ !isEdit ?",
		  "                 (\"${1:PluralEntityName}.AddTitle\" | translate | titlecase)",
		  "                 : (\"${1/(.)/${1:/capitalize}/}.EditTitle\" | translate | titlecase)",
		  "              }}",
		  "            </h2>",
		  "          </mat-panel-title>",
		  "        </mat-expansion-panel-header>",
		  "        <div style=\"margin-top: 1rem;\">",
		  "          <ng-template [ngTemplateOutlet]=\"${2/(.)/${1:/downcase}/}Template\"",
		  "                       [ngTemplateOutletContext]=\"{${2/(.)/${1:/downcase}/}Data:${2/(.)/${1:/downcase}/}Data}\">",
		  "          </ng-template>",
		  "        </div>",
		  "      </mat-expansion-panel>",
		  "",
		  "    </mat-accordion>",
		  "    <!-- end header form -->",
		  "",
		  "    <!-- begin another contents -->",
		  "      <app-training-transaction-list></app-training-transaction-list>",
		  "    <!-- end another contents -->",
		  "",
		  "  </div>",
		  "</ng-container>",
		  "<!--main wrap //-->",
		  "",
		  "",
		  "<!-- begin page header template -->",
		  "<ng-template #${2/(.)/${1:/downcase}/}Template [formGroup]=\"${2/(.)/${1:/downcase}/}Form\" let-data='${2/(.)/${1:/downcase}/}Data'>",
		  "  <!--Form inner-->",
		  "  <div fxLayout=\"column\" fxLayoutAlign=\"start stretch\" fxLayoutGap=\"10px\">",
		  "    <!-- Input fields here -->",
		  "    ${3:InputFields}",
		  "",
		  "",
		  "  <!-- begin submit buttons -->",
		  "  <mat-divider></mat-divider>",
		  "  <div fxLayout=\"row\" fxFlex=\"100\" fxLayoutGap=\"5px\" fxLayoutAlign=\"start none\">",
		  "    <div fxFlex=\"70\"></div>",
		  "    <div fxFlex=\"30\">",
		  "      <button class=\"material-full-width\" *hasClaim=\"['Permissions.${2/(.)/${1:/capitalize}/}.Edit']\" mat-flat-button color=\"primary\" (click)=\"onSubmit()\" [disabled]=\"isSubmitted\">",
		  "        <mat-icon>{{ isSubmitted ? \"pending\" : \"save\"}}</mat-icon>",
		  "        {{ (isSubmitted ? 'Common.Data.SendingData' : 'Common.CRUD.Save') | translate}}",
		  "        <mat-progress-bar *ngIf=\"isSubmitted\" [mode]=\"'indeterminate'\" color=\"accent\"> </mat-progress-bar>",
		  "      </button>",
		  "    </div>",
		  "  </div>",
		  "  <!-- end submit buttons -->",
		  "",
		  "  </div>",
		  "  <!--Form inner //-->",
		  "",
		  "</ng-template>",
		  "<!-- end page header template -->",
		  "",
		  "<ng-template #loading>",
		  "  <div fxLayoutGap=\"10px\" fxLayout=\"column\" fxLayoutAlign=\"start stretch\">",
		  "    <app-shimmer-loading [width]=\"'100%'\" height=\"20px\"></app-shimmer-loading>",
		  "    <app-shimmer-loading [width]=\"'90%'\" height=\"20px\"></app-shimmer-loading>",
		  "    <app-shimmer-loading [width]=\"'75%'\" height=\"20px\"></app-shimmer-loading>",
		  "    <app-shimmer-loading [width]=\"'70px'\" height=\"20px\"></app-shimmer-loading>",
		  "  </div>",
		  "</ng-template>",
		  ""
		],
		"description": "ramez-resolver-form"
	  }, 
	  "ramez-split-main-table-cell": {
