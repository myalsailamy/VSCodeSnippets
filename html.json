		  "    <mat-card-actions>",
		  "      <!--Horizontal button-->",
		  "      <div fxLayoutAlign=\"end center\">",
		  "",
		  "        <div fxLayout=\"row\" fxLayoutAlign=\"center center\" fxLayoutGap=\"10\">",
		  "          <button type=\"reset\" (click)=\"resetForm()\" mat-stroked-button>",
		  "            {{ \"Clear\" | translate | uppercase }}",
		  "          </button>",
		  "          <button type=\"button\" color=\"primary\" mat-flat-button (click)=\"applyFilterClose()\">",
		  "            {{ \"Apply & Close\" | translate | uppercase }}",
		  "          </button>",
		  "        </div>",
		  "",
		  "      </div>",
		  "      <!--Horizontal input //-->",
		  "    </mat-card-actions>",
		  "  </mat-card-footer>",
		  "</mat-card>",
		  ""
		],
		"description": "ramez-filters-form"
	  },
	  "ramez-filters-toolbar-menu": {
		"prefix": "ramez-filters-toolbar-menu",
		"body": [
		  "    <div fxFlex=\"30\" fxFlex.lt-md=\"100\">",
		  "      <div fxLayout=\"row\" fxLayoutGap=\"10px\" fxLayoutAlign=\"none center\">",
		  "        <mat-form-field appearance=\"legacy\" class=\"search-form-field material-full-width\" floatLabel=\"never\">",
		  "          <input matInput placeholder=\"{{'Search' | translate }}\" [formControl]=\"searchKeyCtrl\" />",
		  "          <button mat-button matSuffix mat-icon-button aria-label=\"Clear\" *ngIf=\"searchKeyCtrl.value\" (click)=\"onSearchClear()\">",
		  "            <mat-icon color=\"warn\">close</mat-icon>",
		  "          </button>",
		  "        </mat-form-field>",
		  "        <button mat-icon-button [matMenuTriggerFor]=\"filterMenu\" [matBadge]=\"filterItemCount\" matBadgePosition=\"before\" matBadgeColor=\"accent\">",
		  "          <mat-icon [ngClass]=\" !filterItemCount ? 'material-icons-outlined' : 'material-icons'\">filter_alt</mat-icon>",
		  "        </button>",
		  "      </div>",
		  "    </div>",
		  "  </div>",
		  "  <!-- end table toolbar & search options -->",
		  "",
		  "  <!-- filter-->",
		  "  <mat-menu #filterMenu=\"matMenu\">",
		  "    <ng-template matMenuContent>",
		  "      <div (click)=\"$$event.stopPropagation()\" class=\"filter-wrapper\">",
		  "        <ng-container [formGroup]=\"formFilter\">",
		  "          <app-employee-master-filters (filterCount)=\"getFilterCount($$event)\" (applyFilter)=\"applyMenuFilter($$event)\"",
		  "                                       formControlName=\"filterValues\"></app-employee-master-filters>",
		  "        </ng-container>",
		  "      </div>",
		  "    </ng-template>",
		  "  </mat-menu>",
		  "  <!-- filter //-->"
		],
		"description": "ramez-filters-toolbar-menu"
	  }, 
	  "ramez-tab-group": {
		"prefix": "ramez-tab-group",
		"body": [
		  "<!-- begin tabs group -->",
		  "<mat-tab-group>",
		  "",
		  "  <mat-tab>",
		  "    <ng-template mat-tab-label><mat-icon color=\"primary\">home</mat-icon>&nbsp;&nbsp;{{'Common.Form.Name' | translate }}</ng-template>",
