		  "    <mat-icon [color]=\"${1/(.)/${1:/downcase}/}.${2:PluralColumnNameDownCase} == true ? 'primary' : 'warn'\" class=\"material-icons-outlined\">{{${1/(.)/${1:/downcase}/}.${2/(.)/${1:/downcase}/} == true ? 'check_circle' : 'block' }}</mat-icon>",
		  "    <span>{{(${1/(.)/${1:/downcase}/}.${2/(.)/${1:/downcase}/} == true ? 'Common.CRUD.Enable' : 'Common.CRUD.Disable') | translate}}</span>",
