{
	// Place your snippets for html here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	"mys-HeadLine": {
		"prefix": "mys-HeadLine",
		"body": [
			"<!-- Head Line -->",
			"<div class=\"form-row mt-2\" >",
			  "\t<h6 class=\"horizontal-line text-left h5 px-1 mb-2\">",
				"\t\t<span class=\"head-line-yellow pr-4\"><i class=\"${1:fa fa-cog}\"></i>&nbsp;{{'${2:Common.Data.View}' | translate }}</span>",
			  "\t</h6>",
			"</div>"
		],
		"description": "Head Line"
	},
	"mys-bool-isDisabled": {
		"prefix": "mys-bool-isDisabled",
		"body": [
			"<div class=\"form-row mb-2\">" ,
			"\t<label class=\"col-12 h6 mb-0 text-gold\">" , 
			"\t\t<i class=\"fa fa-retweet\"></i>&nbsp;{{'Common.Form.AllowThisOption' | translate }}:" , 
			"\t\t<span class=\"ml-3 h6 mb-0 text-dark\">[<i class=\"fa\" [ngClass]=\" ${1:entity}.isDisabled == false ? 'fa-check-circle text-success': 'fa-ban text-danger'\"></i> {{(${1:entity}.isDisabled ? 'Common.Form.No' : 'Common.Form.Yes') | translate}}]</span>" , 
			"\t</label>" , 
			"</div>\n"
		],
		"description": "show boolean data as readable information."
	},
	"mys-bool-isAvailable": {
		"prefix": "mys-bool-isAvailable",
		"body": [
			"<div class=\"form-row mb-2\">" ,
			"\t<label class=\"col-12 h6 mb-0 text-gold\">" , 
			"\t\t<i class=\"fa fa-retweet\"></i>&nbsp;{{'Common.Form.AvailableStatus' | translate }}:" , 
			"\t\t<span class=\"ml-3 h6 mb-0 text-dark\">[<i class=\"fa\" [ngClass]=\"{'fa-check-circle text-success': ${1:entity}.isActive == true, 'fa-ban text-danger': ${1:entity}.isactive == false}\"></i> {{(${1:entity}.isActive ? 'Common.Form.Available' : 'Common.Form.NotAvailable') | translate}}]</span>" , 
			"\t</label>" , 
			"</div>\n"
		],
		"description": "show boolean data as readable information."
	},
	"mys-text-note": {
		"prefix": "mys-text-note",
		"body": [
			"<div class=\"form-row\">" , 
			"\t<span class=\"d-flex align-items-center h6 mb-0 text-gold\"><i class=\"fa fa-wpforms\"></i>&nbsp;{{'Common.Form.Note' | translate }}</span>" , 
			"\t<div class=\"col ml-3\"><hr class=\"mt-2 mb-0\"></div>" ,
			"</div>\n" ,
			"<div class=\"form-row mb-2\">" ,
			"\t<div class=\"col-12\">" ,
			"\t\t<ng-container *ngIf=\"${1:entity}?.note;else noTypeContent\">" ,
			"\t\t\t<p class=\"mb-0 text-justify\" style=\"white-space: pre-line\">{{${1:entity}?.note}}</p>" ,
			"\t\t</ng-container>" ,
			"\t\t<ng-template #noTypeContent>" ,
			"\t\t\t<div class=\"alert alert-warning text-center my-2\">" ,
			"\t\t\t\t<strong>{{'Common.notFound.NoContent' | translate }}</strong>" ,
			"\t\t\t</div>" ,
			"\t\t</ng-template>" ,
			"\t</div>" ,
			"</div>\n"
		],
		"description": "show multi line text data as readable information."
	},
	"mys-entity-value": {
		"prefix": "mys-entity-value",
		"body": [
			"<div class=\"form-row mb-2\">" ,
			"\t<label class=\"h6 mb-0 text-info\">" ,
			"\t\t<i class=\"fa fa-info-circle\"></i>&nbsp;{{'Common.Data.MoreInformations' | translate }}:" ,
			"\t\t<span class=\"ml-3 h6 mb-0 text-dark\">[ {{${1:entity}?.id}} ]</span>" ,
			"\t</label>" ,
			"</div>\n"
		],
		"description": "show data as readable information."
	},
	
	"mys-audit-details-template": {
		"prefix": "mys-audit-details-template",
		"body": [
			"<app-audit-details-template [entityData]=\"${1:entity}\"></app-audit-details-template>\n"
		],
		"description": "show data as readable information."
	},

	"mys-large-icon-decoration": {
		"prefix": "mys-large-icon-decoration",
		"body": [
			"<app-large-bg-icon-decoration [iconName]=\"'${1:fa-file-pdf-o}'\" [iconColor]=\"'${2:#c80000}'\" [iconSize]=\"'25rem'\"></app-large-bg-icon-decoration>\n"
		],
		"description": "show data as readable information."
	},

	"mys-table-header": {
		"prefix": "mys-table-header",
		"body": [
			"<thead ngx-table-header class=\"table-primary\" [columns]=\"columns\" [query]=\"query\" (onSortChange)=\"getAll${1:EntityName}()\"></thead>\n"
		],
		"description": "show data as readable information."
	},	
	
	"mys-table-not-found-any-record": {
		"prefix": "mys-table-not-found-any-record",
		"body": [
			"<tr *ngIf=\"!${1:EntityRecords}?.length\" ngx-table-not-found-any-record [entityRecordName]=\"'${1:EntityRecords}.Singular'\" (onAddClicked)=\"show${1:EntityRecords}AddModal()\"></tr>\n"
		],
		"description": "show data as readable information."
	},	
	
	"mys-table-pagination": {
		"prefix": "mys-table-pagination",
		"body": [
			"<ngx-table-pagination [query]=\"query\" (onPageChange)=\"onPageChange($$event);\"></ngx-table-pagination>\n"
		],
		"description": "show data as readable information."
	},	
	
	"mys-table-pagination-with-total": {
		"prefix": "mys-table-pagination-with-total",
		"body": [
			"<ngx-table-pagination-with-total [query]=\"query\" (onPageChange)=\"onPageChange($$event);\"></ngx-table-pagination-with-total>\n"
		],
		"description": "show data as readable information."
	},	

	"mys-table-row-data-container": {
		"prefix": "mys-table-row-data-container",
		"body": [
			"<td [attr.data-title]=\"columns[1]?.title\" class=\"align-middle text-left w-100 py-2\">", 
			"\t<div>", 
			"\t\t<span class=\"h6 my-1\">", 
			"\t\t\t<a [ngClass]=\"${1:EntityName}.isSuspended ? 'text-danger' : 'text-info'\" [routerLink]=\"['/${1:EntityName}', +${1:EntityName}?.id]\"><i class=\"fa\" [ngClass]=\"${1:EntityName}.isSuspended ? 'fa-lock' : 'fa-info-circle'\"></i> {{ ${1:EntityName}.name }}</a>", 
			"\t\t</span>", 
			"\t\t<span class=\"badge badge-secondary px-2 ml-2\">{{${1:EntityName}.description}}</span>", 
			"\t</div>", 
			"\t<!---------- Begin Details ----------->", 
			"\t<div class=\"row row-cols-1 row-cols-sm-2 row-cols-md-5 row-cols-xl-5 no-gutters row-eq-height bg-light mt-1\">", 
			"", 
			"\t\t<div row-data-container [icon]=\"'fa-calendar-o'\" [caption]=\"'Common.Data.UpdatedDate' | translate\" [value]=\"${1:EntityName}?.updatedDate| date:'yyyy/MM/dd' || '~~~~'\" [hideBorder]=\"true\"></div>", 
			"", 
			"\t</div>", 
			"\t<!---------- Begin Details ----------->", 
			"</td>"
		],
		"description": "show data as readable information."
	},	

	"mys-table-row-data-cell": {
		"prefix": "mys-table-row-data-cell",
		"body": [
			"<div row-data-container [icon]=\"'fa-info-circle'\" [caption]=\"'Common.Data.Record' | translate\"",
			"\t\t\t\t\t\t\t[value]=\"${1:EntityRecords}.${2:EntityPropertyName} || '~~~~'\" [hideInMobile]=\"true\"></div>\n"
		],
		"description": "show data as readable information."
	},	
	"mys-table-auto-number": {
		"prefix": "mys-table-column-auto-number",
		"body": [
			"<td [attr.data-title]=\"'Common.Form.Id' | translate\" class=\"align-middle text-center py-0\">{{(query.pageSize * (query.pageNumber - 1)) + i + 1}}</td>\n"
		],
		"description": "show data as readable information."
	},	
	"mys-model-header-add-or-update": {
		"prefix": "mys-model-header-add-or-update",
		"body": [
			"<h5 class=\"modal-title text-truncate\">", 
			"\t<span *ngIf=\"!${1:EntityRecords}\"><i class=\"fa fa-plus\"></i>&nbsp;{{'${2:EntityRecords}.AddTitle' | translate }}</span>", 
			"\t<span *ngIf=\"+${1:EntityRecords}?.id > 0\"><i class=\"fa fa-pencil-square-o\"></i>&nbsp;{{'${2:EntityRecords}.EditTitle' | translate }}</span>", 
			"\t<div *ngIf=\"+${1:EntityRecords}?.id > 0\" class=\"mt-2 px-3 bg-primary text-light text-truncate\">", 
			"\t\t<small><i class=\"fa fa-arrow-left\" aria-hidden=\"true\"></i> {{'sub-title'}}</small>", 
			"\t</div>", 
			"</h5>\n"
		],
		"description": "show data as readable information."
	},		
	"mys-corner-menu": {
		"prefix": "mys-corner-menu",
		"body": [
			"<div class=\"dropdown float-left\"><a class=\"nav-link dropdown-toggle arrow-none\" data-toggle=\"dropdown\" data-display=\"static\" href=\"return;\" role=\"button\" aria-haspopup=\"false\" aria-expanded=\"false\"><i class=\"fa fa-ellipsis-v fa-1x text-muted\"></i></a>" ,
			"\t<div class=\"dropdown-menu dropdown-menu-right\">" ,
			"\t\t<button type=\"button\" class=\"dropdown-item px-2\" (click)=\"updateAlternativeItemsOrder()\">" ,
			"\t\t\t<i class='text-success fa ' [ngClass]=\"isSubmitting ? 'fa-circle-o-notch fa-spin' : 'fa-sort-amount-desc'\"></i>" ,
			"\t\t\t<span>&nbsp;{{(isSubmitting ? 'Common.Data.SendingData' : 'Common.CRUD.Reorder') | translate}}</span>" ,
			"\t\t</button>" ,
			"\t</div>" ,
			"</div>\n"
		],
		"description": "show data as readable information."
	},
	"mys-tool-tip": {
		"prefix": "mys-tool-tip",
		"body": [
			"<i class=\"fa fa-question-circle text-info\" placement=\"top\" ngbTooltip=\"{{'Items.TouchScreenTips' | translate }}\" [openDelay]=\"100\" [closeDelay]=\"1000\"></i>"
		],
		"description": "show data as tool tip to help user."
	},	
	"mys-last-update-table-column": {
		"prefix": "mys-last-update-table-column",
		"body": [
			"<td class=\"align-middle text-center\" [title]=\"${1:entityName}.updatedDate| date:'yyyy/MM/dd hh:mm a'\">{{${1:entityName}.updatedDate| timeAgo}}</td>"
		],
		"description": "show data as tool tip to help user."
	},
	"mys-tabs": {
		"prefix": "mys-tabs",
		"body": [
		  "  <!-------------------------- Begin Tabs  ------------------------------>",
		  "  <ul ngbNav #nav=\"ngbNav\" [(activeId)]=\"activeNavTab\" class=\"nav nav-tabs\">",
		  "",
		  "    <li [ngbNavItem]=\"1\">",
		  "      <a ngbNavLink class=\"px-2\"><i class=\"fa fa-users\" aria-hidden=\"true\"></i> {{'Councils.Edit' | translate:{'value':''} }}</a>",
		  "      <ng-template ngbNavContent>",
		  "        <ng-template [ngTemplateOutlet]=\"ModifyCouncilForm\"></ng-template>",
		  "      </ng-template>",
		  "    </li>",
		  "",
		  "    <li [ngbNavItem]=\"2\" *ngIf=\"councilForEdit?.userPermissions?.canMangeContentCoordinatorMembers\">",
		  "      <a ngbNavLink class=\"px-2\"><i class=\"fa fa-pencil-square-o\" aria-hidden=\"true\"></i> {{'Councils.Members.AddContentCoordinator' | translate}}</a>",
		  "      <ng-template ngbNavContent>",
		  "        <app-add-content-coordinator-member [councilOrCommitteeId]=\"councilForEdit.id\" [councilOrCommitteeDetails]=\"councilForEdit\" [showAsModal]=\"false\"></app-add-content-coordinator-member>",
		  "      </ng-template>",
		  "    </li>",
		  "",
		  "    <li [ngbNavItem]=\"3\" *ngIf=\"councilForEdit?.userPermissions?.canMangeApproverMembers\">",
		  "      <a ngbNavLink class=\"px-2\"><i class=\"fa fa-thumbs-up\" aria-hidden=\"true\"></i> {{'Councils.AddApproverMember' | translate:{'value': ''} }}</a>",
		  "      <ng-template ngbNavContent>",
		  "        <app-add-approver-member *ngIf=\"councilForEdit?.userPermissions?.canMangeApproverMembers\" [councilOrCommitteeId]=\"councilForEdit.id\" [councilOrCommitteeDetails]=\"councilForEdit\" [showAsModal]=\"false\"></app-add-approver-member>",
		  "      </ng-template>",
		  "    </li>",
		  "",
		  "  </ul>",
		  "  <div [ngbNavOutlet]=\"nav\"></div>",
		  "  <!-------------------------- End Tabs  ------------------------------>"
		],
		"description": "mys-tabs"
	  },








	
	"ramez-from-dialog-subtitle": {
		"prefix": "ramez-from-dialog-subtitle",
		"body": [
			"<div *ngIf=\"(${2:SingularEntityNameDowncase}Info$ | async)  as ${2/(.)/${1:/downcase}/};\" fxLayout=\"row\" fxLayoutAlign=\"space-between center\">",
			"  <h2 class=\"mb-0\">&nbsp;&nbsp;[<strong class=\"primary-color\">{{${2/(.)/${1:/downcase}/}?.data?.employee?.value || '~~~~~ ~~~~~ ~~~~~'}}</strong>]</h2>",
			"</div>"
		],
		"description": "ramez-from-dialog-subtitle"
	},
	"ramez-form-html-component": {
		"prefix": "ramez-form-html-component",
		"body": [
		  "<div fxLayout=\"row\" class=\"popup-top\" fxLayoutAlign=\"space-between center\">",
		  "  <mat-icon fxFlex=\"none\" color=\"primary\">{{!isEdit ? 'add' : 'drive_file_rename_outline'}}</mat-icon>&nbsp;&nbsp;",
		  "  <h2 fxFlex style=\"margin-bottom: 0px;\">",
		  "    {{ !isEdit ?",
		  "       (\"${1:PluralEntityNameCapitalize}.AddTitle\" | translate | titlecase)",
		  "       : (\"${1/(.)/${1:/capitalize}/}.EditTitle\" | translate | titlecase)",
		  "    }}",
		  "  </h2>",
		  "  <button fxFlex=\"none\" mat-icon-button color=\"warn\" type=\"button\" tabIndex=\"-1\" [mat-dialog-close]=\"false\">",
		  "    <mat-icon>close</mat-icon>",
		  "  </button>",
		  "</div>",
		  "<mat-divider></mat-divider>",
		  "  <ng-container *ngIf=\"(${2:SingularEntityNameDowncase}Info$ | async) || !isEdit; else loading\">",
		  "    <form class=\"form-wrapper\" (ngSubmit)=\"onSubmit()\" [formGroup]=\"${2/(.)/${1:/downcase}/}Form\" autocomplete=\"off\">",
		  "      <mat-dialog-content class=\"mat-typography\">",
		  "        <!--Main card-->",
		  "        <mat-card class=\"mat-elevation-z0\">",
		  "          <!--Form inner-->",
		  "          <div fxLayout=\"column\" fxLayoutAlign=\"start stretch\" fxLayoutGap=\"10px\">",
		  "",
		  "            <!-- Input fields here -->",
		  "            ${3:InputFields}",
		  "",
		  "          </div>",
		  "          <!--Form inner//-->",
		  "        </mat-card>",
		  "        <!--Main card-->",
		  "      </mat-dialog-content>",
		  "",
		  "      <!--Buttons-->",
		  "      <mat-dialog-actions>",
		  "        <div fxLayout=\"row\" fxFlex=\"100\" fxLayoutGap=\"5px\" fxLayoutAlign=\"start none\">",
		  "          <div fxFlex=\"50\">",
		  "            <button type=\"button\" class=\"material-full-width popup-action-btn\" mat-stroked-button color=\"warn\" [mat-dialog-close]=\"false\">",
		  "              <mat-icon>block</mat-icon>",
		  "              {{ \"Common.CRUD.Cancel\" | translate | uppercase }}",
		  "            </button>",
		  "          </div>",
		  "          <div fxFlex=\"50\">",
		  "            <button class=\"material-full-width large-button\" mat-flat-button color=\"primary\" type=\"submit\" [disabled]=\"isSubmitted\">",
		  "              <mat-icon class=\"mat-18\">{{ isSubmitted ? \"pending\" : \"save\"}}</mat-icon>",
		  "              {{ (isSubmitted ? 'Common.Data.SendingData' : 'Common.CRUD.Save') | translate}}",
		  "              <mat-progress-bar *ngIf=\"isSubmitted\" [mode]=\"'indeterminate'\" color=\"accent\"> </mat-progress-bar>",
		  "            </button>",
		  "          </div>",
		  "        </div>",
		  "      </mat-dialog-actions>",
		  "    </form>",
		  "  </ng-container>",
		  "",
		  "  <ng-template #loading>",
		  "    <!-- <mat-progress-bar [mode]=\"'indeterminate'\" color=\"warn\"></mat-progress-bar>-->",
		  "    <div fxLayout=\"row\" fxLayoutAlign=\"space-between start\" fxLayoutAlign.lt-lg=\"colum\" fxLayoutGap=\"20px\" fxLayoutAlign.lt-lg=\"start stretch\">",
		  "      <!--Left-->",
		  "      <div fxFlex=\"50%\" fxLayoutGap=\"10px\" fxLayout=\"column\" fxLayoutAlign=\"start stretch\">",
		  "        <app-shimmer-loading [width]=\"'100%'\" height=\"40px\"></app-shimmer-loading>",
		  "        <app-shimmer-loading [width]=\"'100%'\" height=\"40px\"></app-shimmer-loading>",
		  "        <app-shimmer-loading [width]=\"'100%'\" height=\"40px\"></app-shimmer-loading>",
		  "        <app-shimmer-loading [width]=\"'70px'\" height=\"20px\"></app-shimmer-loading>",
		  "      </div>",
		  "      <!--Left //-->",
		  "      <!--Right-->",
		  "      <div fxFlex=\"50%\" fxLayoutGap=\"10px\" fxLayout=\"column\" fxLayoutAlign=\"start stretch\">",
		  "        <app-shimmer-loading [width]=\"'100%'\" height=\"40px\"></app-shimmer-loading>",
		  "        <app-shimmer-loading [width]=\"'100%'\" height=\"40px\"></app-shimmer-loading>",
		  "        <app-shimmer-loading [width]=\"'100%'\" height=\"40px\"></app-shimmer-loading>",
		  "      </div>",
		  "      <!--Right //-->",
		  "    </div>",
		  "  </ng-template>"
		],
		"description": "ramez-form-html-component"
	  },
	  "ramez-sort-by-language": {
		"prefix": "ramez-sort-by-language",
		"body": [
		  "[mat-sort-header]=\"dir == 'rtl' ? '${1:SingularEntityNameDowncase}Ar' : '${1:SingularEntityNameDowncase}'\""
		],
		"description": "ramez-sort-by-language"
	  },
	  "ramez-boolean-value-as-an-icon": {
