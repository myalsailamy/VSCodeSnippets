		  "              <div *ngIf=\"${2/(.)/${1:/downcase}/}.remark\" fxFlex=\"100\" class=\"note mat-body-1\">",
		  "                <strong><mat-icon class=\"mat-18 icon-sm\">sticky_note_2</mat-icon>  {{'Common.Form.Remarks' | translate }}:</strong>",
		  "                {{${2/(.)/${1:/downcase}/}.remark}}",
		  "              </div>",
		  "",
		  "            </div>",
		  "            <!-- end split main table cell -->"
