	  "ramez-table-expand-row": {
		"prefix": "ramez-table-expand-row",
		"body": [
		  "<ng-container matColumnDef=\"actions1\">",
		  "  <mat-header-cell *matHeaderCellDef>{{ \"Actions\" | translate }}</mat-header-cell>",
		  "  <mat-cell *matCellDef=\"let ${1:SingularEntityNameDowncase}\">",
		  "    <button mat-mini-fab color=\"primary\" (click)=\"${1:SingularEntityNameDowncase}.isExpanded = !${1:SingularEntityNameDowncase}.isExpanded\" class=\"mat-elevation-z0\">",
		  "      <mat-icon >{{${1:SingularEntityNameDowncase}.isExpanded ? 'compress' : 'expand_content'}}</mat-icon>",
		  "    </button>",
		  "  </mat-cell>",
		  "</ng-container>",
		  "",
		  "",
		  "<!-- 1) add ( multiTemplateDataRows ) to  mat-table -->",
		  "<!-- 2) add isExpanded property to row dataType -->",
		  "<mat-row *matRowDef=\"let ${1:SingularEntityNameDowncase}; columns: displayedColumns\"",
		  "    class=\"table-element-row\" [class.table-expanded-row]=\"${1:SingularEntityNameDowncase}.isExpanded\"",
		  "    (click)=\"${1:SingularEntityNameDowncase}.isExpanded = !${1:SingularEntityNameDowncase}.isExpanded\">",
		  "</mat-row>",
		  "<mat-row *matRowDef=\"let row; columns: ['expandedDetail']\" class=\"table-detail-row\"></mat-row>",
		  "<!--  begin expanded detail -->",
		  "<ng-container matColumnDef=\"expandedDetail\">",
		  "  <mat-cell *matCellDef=\"let ${1:SingularEntityNameDowncase}\" [attr.colspan]=\"displayedColumns.length\">",
		  "      <div fxFlex class=\"table-element-detail\" [@detailExpand]=\"${1:SingularEntityNameDowncase}.isExpanded ? 'expanded' : 'collapsed'\">",
		  "        <div fxLayout=\"row\" fxLayoutAlign=\"stretch\" fxLayoutGap=\"5px\">",
		  "          <div fxFlex=\"none\">",
		  "            <mat-icon color=\"primary\" class=\"mat-18\"> {{ dir == 'rtl' ? 'subdirectory_arrow_left' :  'subdirectory_arrow_right'}}</mat-icon>",
		  "          </div>",
		  "          <div class=\"expanded-contents\" fxFlex=\"100\" fxLayout=\"column\" fxLayoutAlign=\" stretch\" fxLayoutGap=\"10px\">",
		  "",
		  "            <div fxFlex=\"100\" fxFlexFill fxLayout=\"row wrap\" fxLayoutAlign=\"space-between stretch\" fxLayoutGap=\"10px grid\">",
		  "              <div fxFlex=\"50\" fxLayout=\"row\" fxLayoutAlign=\"space-between stretch\" fxLayout.lt-md=\"column\" fxLayoutAlign.lt-md=\"start stretch\" fxLayoutGap=\"10px\">",
		  "                <div fxFlex=\"50\">",
		  "                  <mat-label class=\"mat-caption\"><mat-icon class=\"icon-sm\">123</mat-icon> {{'Common.Form.Id' | translate }}</mat-label>",
		  "                  <div class=\"mat-body\">",
		  "                    <strong>{{${1:SingularEntityNameDowncase}.recno || '~~~~~~~~'}}</strong>",
		  "                  </div>",
		  "                </div>",
		  "                <div fxFlex=\"50\">",
		  "                  <mat-label class=\"mat-caption\"><mat-icon class=\"icon-sm\">info</mat-icon> {{'Common.Form.Name' | translate }}</mat-label>",
		  "                  <div class=\"mat-body\">",
		  "                    <strong>{{${1:SingularEntityNameDowncase}.name || '~~~~~~~~'}}</strong>",
		  "                  </div>",
		  "                </div>",
		  "              </div>",
		  "              <div fxFlex=\"50\" fxLayout=\"row\" fxLayoutAlign=\"space-between stretch\" fxLayout.lt-md=\"column\" fxLayoutAlign.lt-md=\"start stretch\" fxLayoutGap=\"10px\">",
		  "                <div fxFlex=\"50\">",
		  "                  <mat-label class=\"mat-caption\"><mat-icon class=\"icon-sm\">calendar_month</mat-icon> {{'EmpDocuments.IssueDate' | translate }}</mat-label>",
		  "                  <div class=\"mat-body\">",
		  "                    <strong>{{(${1:SingularEntityNameDowncase}.issueDate | date:'yyyy/MM/dd') || '~~~~~~~~'}}</strong>",
		  "                  </div>",
		  "                </div>",
		  "                <div fxFlex=\"50\">",
		  "                  <mat-label class=\"mat-caption\"><mat-icon class=\"icon-sm\">calendar_month</mat-icon> {{'EmpDocuments.ExpiryDate' | translate }}</mat-label>",
		  "                  <div class=\"mat-body\">",
		  "                    <strong>{{(${1:SingularEntityNameDowncase}.expiryDate | date:'yyyy/MM/dd') || '~~~~~~~~'}}</strong>",
		  "                  </div>",
		  "                </div>",
		  "              </div>",
		  "            </div>",
		  "",
		  "          </div>",
		  "        </div>",
		  "      </div>",
		  "  </mat-cell>",
		  "</ng-container>",
		  "<!--  end expanded detail -->"
		],
		"description": "ramez-table-expand-row",
	  }, 
	  "ramez-filters-form": {
		"prefix": "ramez-filters-form",
		"body": [
		  "<mat-card [formGroup]=\"form\" class=\"mat-elevation-z0\" fxFlex=\"100\">",
		  "  <mat-card-content >",
		  "    <div fxLayout=\"column\" fxLayoutGap=\"0px\" fxLayoutAlign=\"start stretch\">",
		  "      <!--Horizontal input-->",
		  "",
		  "    </div>",
		  "",
		  "  <!--Main col //-->",
		  "  <!-- {{ form.value | json }} -->",
		  "  </mat-card-content>",
		  "  <mat-divider></mat-divider>",
